{
  "name": "parcel-react-tailwind-template",
  "version": "0.0.1",
  "main": "index.html",
  "scripts": {
    "start": "MODE=development parcel serve src/index.html",
    "build": "MODE=production parcel build src/index.html --no-scope-hoist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tehpsalmist/parcel-react-tailwind-template.git"
  },
  "keywords": [
    "parcel",
    "react",
    "tailwindcss",
    "template"
  ],
  "author": "Ben Steward <bstewardcodes@gmail.com> (https://tehpsalmist.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tehpsalmist/parcel-react-tailwind-template/issues"
  },
  "homepage": "https://github.com/tehpsalmist/parcel-react-tailwind-template#readme",
  "devDependencies": {
    "@parcel/transformer-svg-react": "^2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "autoprefixer": "latest",
    "parcel": "latest",
    "prettier-plugin-tailwindcss": "^0.2.6",
    "tailwindcss": "latest"
  },
  "dependencies": {
    "@8thday/react": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "app": "public/index.html",
  "targets": {
    "main": false,
    "app": {
      "engines": {
        "browsers": "defaults"
      }
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}